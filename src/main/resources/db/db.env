# Spring Boot 启用的运行环境（如 docker、dev）。
SPRING_PROFILES_ACTIVE=dev
# 应用连接数据库的用户名（建议使用非 root）。
DB_USERNAME=root
# 应用连接数据库的密码。
DB_PASSWORD=123456
# 应用数据源的 JDBC URL。在 docker-compose 中，grid-mysql 是数据库服务名。
DB_URL=jdbc:mysql://localhost:3306/GridDB?allowPublicKeyRetrieval=true&useSSL=false&autoReconnect=true&failOverReadOnly=false&maxReconnects=10
# 数据库容器的 root 密码。
MYSQL_ROOT_PASSWORD: spring
# 容器启动时创建的数据库名称。
MYSQL_DATABASE: GridDB
# 容器启动时创建的数据库用户。
MYSQL_USER: root
# 上述数据库用户的密码。
MYSQL_PASSWORD: 123456
# phpMyAdmin 连接的目标主机名（docker-compose 中的数据库服务名）。
PMA_HOST: mysql
# phpMyAdmin 连接的目标端口。
PMA_PORT: 3306
